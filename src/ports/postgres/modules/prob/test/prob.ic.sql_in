/* -----------------------------------------------------------------------------
 * Test probability functions.
 * -------------------------------------------------------------------------- */

SELECT
    MADLIB_SCHEMA.bernoulli_cdf(0,   0.3),
    MADLIB_SCHEMA.beta_cdf(-1, 1, 1) ,
    MADLIB_SCHEMA.binomial_cdf(11, 11, 0.4),
    MADLIB_SCHEMA.cauchy_cdf('-Inf', 1, 2),
    MADLIB_SCHEMA.chi_squared_cdf(-1, 1) ,
    MADLIB_SCHEMA.exponential_cdf(-1, 1),
    MADLIB_SCHEMA.extreme_value_cdf(3, 1, 2),
    MADLIB_SCHEMA.fisher_f_cdf(0, 1, 1),
    MADLIB_SCHEMA.gamma_cdf(0, 0.5, 2) ,
    MADLIB_SCHEMA.geometric_cdf(0,   0.3) ,
    MADLIB_SCHEMA.hypergeometric_cdf(-1 ,50, 30, 500),
    MADLIB_SCHEMA.inverse_gamma_cdf(-1, 1, 2),
    MADLIB_SCHEMA.laplace_pdf('-Inf', 1, 1),
    MADLIB_SCHEMA.logistic_quantile(0.7310586, 1, 2),
    MADLIB_SCHEMA.normal_quantile(NULL, 0, 1),
    MADLIB_SCHEMA.poisson_cdf(NULL, 1),
    MADLIB_SCHEMA.uniform_pdf(-1, 0, 1)
;
